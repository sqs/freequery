	function checksearch()
	{
	  var word=document.hyformnew.keywords.value;
	if(word.replace(/\s+/g,"")==""||word.substring(0,3)=='请输入')
	{
	alert('请输入信息关键字！');
	document.hyformnew.keywords.value="";
	document.hyformnew.keywords.focus();
	
 	return false;
	 }
	}
	</script>

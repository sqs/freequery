 function chkfrm(f){
     if (!f.u.value) return vErr(f.u,'请填写用户名！');
     if (!f.p.value) return vErr(f.p,'填写会员登录密码！');
}
function shearchfrm(f){
if (!f.ickw.value) return vErr(f.ickw,'输入查询IC关键字！');
if (f.ickw.value.length < 3) return vErr(f.ickw,'你输入的关键字太短！');
}

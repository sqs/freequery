	<head>
		<meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
		<meta name="Author" content="www.ins1000.cn,ins1000,zhangshijian,lixi" />
		<title>蹇璁缃</title>
		<link href="images/style.css" rel="stylesheet" type="text/css" />
		<script type="text/javascript" src="js/jquery.js"></script>
		<script src="http://un.so.gougou.com/js/SearchView.js" charset="gbk"></script>
		<script type="text/javascript" src="js/jquery.cookie.js"></script>
		<script type="text/javascript" src="js/mail.js"></script>
		<script type="text/javascript">
			document.oncontextmenu=function(){event.returnValue=false;}
            var flag = true;
            $(document).ready(function(){
            
                var bcs = ["best-navi", "query", "soft", "site", "game", "mail", "visits"];
                var current = 1;
                $('#best-cates li').click(function(){
                    if (this.id.substr(2) == current) 
                        return;
                    $('#bc' + current).removeClass('cur');
                    $('#' + bcs[current - 1]).css('display', 'none');
                    current = this.id.substr(2);
                    $(this).addClass('cur');
                    $('#' + bcs[current - 1]).css('display', 'block');
                });
				show();
			$("a").click(function(){
				if (flag) {
                        var historys = $.cookie("historys");
                        var text = $(this).text();
						var url = $(this).get(0);
						if (text == null || text == "") {
                            return;
                        }
						text = "<li>" + "<a href='" + url + "' target='_blank'>" + text + "</a>" + "</li>";
                        if (historys == null) {
                            $.cookie("historys", text, {
                                expires: 8
                            });
                            $("#rs").append(text);
                        }
                        else {
                            historys = historys + "," + text;
                            $.cookie("historys", historys, {
                                expires: 8
                            });
                            show();
                        }
                    }
				});
            });
			function show() {
				$("#rs").html("");
				var historys = $.cookie("historys");
				if (historys != null) {
					var history = historys.split(",");
					var i = 0;
					while (i < history.length) {
						$("#rs").append(history[i]);
						i++
					}
				}				
			}
			function reset() {
				$.cookie("historys", null);
				$("#rs").html("");
			}
			function change() {
				 if (flag == false) {
                    flag = true;
                    $("#change").html("姝㈣板");
                }
                else 
                    if (flag == true) {
                        flag = false;
                        $("#change").html("寮濮璁板");
                    }
			}
        </script>
	</head>
